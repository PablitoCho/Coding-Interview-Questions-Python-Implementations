    "def k_strings(n, k, string):\n",
    "    if n==0:\n",
    "        print(''.join(string))\n",
    "    else:\n",
    "        for i in range(k):\n",
    "            string[-n] = str(i)\n",
    "            k_strings(n-1, k,  string)"
   ]
